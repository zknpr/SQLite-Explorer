{
  "$schema": "https://json.schemastore.org/vercel.json",
  "framework": "nextjs",
  "regions": ["iad1"],
  "headers": [
    {
      "source": "/(.*)\\.(js|css|woff2?|png|jpg|jpeg|gif|ico|svg|db)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
